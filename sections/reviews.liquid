{% schema %}
{
  "name": "Lumin Review",
 "settings": [
   {
        "type": "header",
        "content": "Note",
        "info": "This review section is for demostration purpose. If you want to collect reviews from customers, please consider installing any review app from Shopify [App Store](https:\/\/apps.shopify.com\/)"
      },
   {
      "type": "color_scheme",
      "id": "color_scheme",
      "label": "t:sections.all.colors.label",
      "default": "background-1"
    },
   {
        "type": "header",
        "content": "Top Head"
      },
{
        "type": "text",
        "id": "header_text",
        "label": "Header",
        "default": "what our customers say about us"
      },
   {
        "type": "checkbox",
        "id": "show_top",
        "label": "Show top Head",
        "default": true
      },
    {
        "type": "checkbox",
        "id": "show_footer",
        "label": "Show Review Footer",
        "default": true
      },
{
        "type": "inline_richtext",
        "id": "rating",
        "label": "Rating",
        "default": "4.9"
      },
{
        "type": "text",
        "id": "rating_count",
        "label": "Review count",
        "default": "16",
        "info": "Remove to hide"
      },
   {
        "type": "header",
        "content": "Section Settings"
      },
{
        "type": "text",
        "id": "review_column_mobile",
        "label": "Review column Mobile",
        "default": "1"
      },
{
        "type": "text",
        "id": "review_column_tab",
        "label": "Review column Tablet",
        "default": "1"
      },
{
        "type": "text",
        "id": "review_column_desk",
        "label": "Review column Desktop",
        "default": "1"
      },
   {
        "type": "text",
        "id": "review_load",
        "label": "Load more",
        "default": "4"
      },
    {
        "type": "header",
        "content": "Sizes"
      },
      {
    "type":      "range",
    "id":        "rw_img_size",
    "min":       20,
    "max":        400,
    "step":       10,
    "unit":       "px",
    "label":     "Review Image size",
    "default":   140
 },
     {
    "type":      "range",
    "id":        "rw_img_radius",
    "min":       0,
    "max":        100,
    "step":       1,
    "unit":       "px",
    "label":     "Image Radius",
    "default":   0
 },

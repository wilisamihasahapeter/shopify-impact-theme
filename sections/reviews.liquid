			<script type="text/javascript">
          $(document).ready(function(){
            $(".review-link").click(function(){
              $('html,body').animate({
                scrollTop: $('#rw-top').offset().top
              }, 500);
          
              return false;
            });
          })
      </script>
			<script>
          $(document).ready(function(){
          $(".rw-container-contant").slice(0, {{section.settings.review_load}}).show();
          $("#loadMore").on("click", function(e){
            e.preventDefault();
            $(".rw-container-contant:hidden").slice(0, {{section.settings.review_load}}).slideDown();
            if($(".rw-container-contant:hidden").length == 0) {
              $("#loadMore").text("No Content").addClass("noContent");
            }
          });
          
          })
